{
  "multipart": [
    {
      "when": {
        "axis": "x"
      },
      "apply": {
        "model": "ilikewood:block/post/stripped/redwood",
        "x": 90,
        "y": 90
      }
    },
    {
      "when": {
        "axis": "y"
      },
      "apply": {
        "model": "ilikewood:block/post/stripped/redwood",
        "x": 0,
        "y": 0
      }
    },
    {
      "when": {
        "axis": "z"
      },
      "apply": {
        "model": "ilikewood:block/post/stripped/redwood",
        "x": 90,
        "y": 0
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "ilikewood:block/post/stripped/side/redwood",
        "x": 270,
        "y": 0,
        "uvlock": false
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "ilikewood:block/post/stripped/side/redwood",
        "x": 90,
        "y": 0,
        "uvlock": false
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "ilikewood:block/post/stripped/side/redwood",
        "x": 0,
        "y": 0,
        "uvlock": false
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "ilikewood:block/post/stripped/side/redwood",
        "x": 0,
        "y": 90,
        "uvlock": false
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "ilikewood:block/post/stripped/side/redwood",
        "x": 0,
        "y": 180,
        "uvlock": false
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "ilikewood:block/post/stripped/side/redwood",
        "x": 0,
        "y": 270,
        "uvlock": false
      }
    }
  ]
}
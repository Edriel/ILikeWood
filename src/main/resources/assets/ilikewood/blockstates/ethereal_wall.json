{
  "multipart": [
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "ilikewood:block/wall/post/ethereal"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "ilikewood:block/wall/side/ethereal",
        "y": 0,
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "ilikewood:block/wall/side/ethereal",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "ilikewood:block/wall/side/ethereal",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "ilikewood:block/wall/side/ethereal",
        "y": 270,
        "uvlock": true
      }
    }
  ]
}
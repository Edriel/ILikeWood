{
    "multipart": [
        {   "when": { "up": "true" },
            "apply": { "model": "ilikewood:block/wall/post/jungle" }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "ilikewood:block/wall/side/jungle", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "ilikewood:block/wall/side/jungle", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "ilikewood:block/wall/side/jungle", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "ilikewood:block/wall/side/jungle", "y": 270, "uvlock": true }
        }
    ]
}

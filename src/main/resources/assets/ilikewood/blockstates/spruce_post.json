{
	"multipart": [
		{
			"when": { "axis": "y" },
			"apply": { "model": "ilikewood:block/post/spruce" }
		},
		{
			"when": { "axis": "x" },
			"apply": { "model": "ilikewood:block/post/spruce", "y": 90, "x": 90 }
		},
		{
			"when": { "axis": "z" },
			"apply": { "model": "ilikewood:block/post/spruce", "y": 0, "x": 90 }
		},
		{   "when": { "north": true },
				"apply": { "model": "ilikewood:block/post/side/spruce" }
		},
		{   "when": { "east": true },
				"apply": { "model": "ilikewood:block/post/side/spruce", "y": 90, "uvlock": false }
		},
		{   "when": { "south": true },
				"apply": { "model": "ilikewood:block/post/side/spruce", "y": 180, "uvlock": false }
		},
		{   "when": { "west": true },
				"apply": { "model": "ilikewood:block/post/side/spruce", "y": 270, "uvlock": false }
		},
		{   "when": { "up": true },
				"apply": { "model": "ilikewood:block/post/side/spruce", "x": 270, "uvlock": false }
		},
		{   "when": { "down": true },
				"apply": { "model": "ilikewood:block/post/side/spruce", "x": 90, "uvlock": false }
		}
	]
}
